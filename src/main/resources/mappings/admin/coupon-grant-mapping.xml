<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="UserCouponDAO">
	
	
	<select id="getUserIDList" resultType="CouponGrantVO">
		select user_id from this_user
	</select>
	
	<select id="getAdminCoupon" resultType="CouponGrantVO">
		select * from admin_coupon
	</select>
	
	<insert id="userGrantCoupon">
		insert into user_coupon(user_id, coupon_no, coupon_name, coupon_price, coupon_first, coupon_last)
		values(#{user_id},#{coupon_no},#{coupon_name},#{coupon_price},#{coupon_first},#{coupon_last})
	</insert>
	
	<!-- 수정, 삭제, 상세보기 , 조회, -->
	
	<select id="userCouponList" resultType="CouponGrantVO">
		select * from user_coupon
	</select>
	
	
	
</mapper>
